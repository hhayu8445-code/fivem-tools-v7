# FiveCFX Decrypt Bot - Environment Variables Example
# Copy this file to .env and fill in your values
# Note: Using config.js is recommended over .env

# Discord Bot Configuration
DISCORD_TOKEN=YOUR_DISCORD_BOT_TOKEN
DISCORD_CLIENT_ID=YOUR_CLIENT_ID
DISCORD_GUILD_ID=YOUR_GUILD_ID
DISCORD_LOG_CHANNEL_ID=YOUR_LOG_CHANNEL_ID
DISCORD_INVITE=https://discord.gg/YOURCODE

# Server Configuration
APP_URL=http://localhost:3000/
WEB_PORT=3000
API_PORT=3001

# Admin Configuration (comma-separated Discord User IDs)
ADMIN_USERS=123456789012345678,987654321098765432

# Proxy Configuration (optional)
PROXY_ENABLED=false
PROXY_HOST=
PROXY_PORT=
PROXY_USERNAME=
PROXY_PASSWORD=

# Security (optional - leave empty for defaults)
# SESSION_SECRET=
# JWT_SECRET=
